<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="750.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.uniquindio.semester7.controller.MenuPaneController">
   <children>
      <GridPane prefHeight="759.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="ALWAYS" maxWidth="395.0" minWidth="10.0" prefWidth="193.0" />
          <ColumnConstraints hgrow="ALWAYS" maxWidth="612.0" minWidth="10.0" prefWidth="607.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <HBox alignment="CENTER" prefHeight="63.0" prefWidth="200.0" spacing="20.0">
                     <children>
                        <Label text="Super Admin" />
                        <Label fx:id="labelName" text="Crisi" />
                     </children>
                  </HBox>
                  <TreeView fx:id="treeViewElements" onContextMenuRequested="#handleClickedTreeItem" onMouseClicked="#handleClickedTreeItem" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <BorderPane fx:id="modifyPane" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
         </children>
      </GridPane>
   </children>
</VBox>
